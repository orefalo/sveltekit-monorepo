{
	"name": "my-turborepo",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"packages/*",
		"tooling/*"
	],
	"scripts": {
		"build": "turbo run build",
		"start": "turbo run dev --parallel",
		"dev": "pnpm run start",
		"check": "turbo run check",
		"lint": "turbo run lint",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier-config-custom": "workspace:*",
		"turbo": "1.6.3"
	},
	"engines": {
		"npm": ">=7.0.0",
		"pnpm": ">=7",
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.12.1",
	"license": "MIT"
}
